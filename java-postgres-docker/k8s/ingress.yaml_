apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: mi-aplicacion-backend-ingress
  annotations:
    kubernetes.io/ingress.class: "nginx"
spec:
  rules:
  - host: ingressback.duckdns.org
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: mi-aplicacion-backend
            port:
              number: 80